import unittest 
from decimal import Decimal

class targetAndLoggedMacros(unittest.TestCase):

	def test_MacroTracking({‘fat’: Decimal('77'), ‘protein’: Decimal('53'), ‘carbs’: Decimal('56')})
		tracker.addMacro(‘fat’, Decimal('77'))
		tracker.addMacro(‘protein’, Decimal('53'))
		tracker.addMacro(‘carbs’, Decimal('56'))
